<VirtualHost *:80>
   ServerAdmin contact@french-gaming-family.ga
   ServerName french-gaming-family.ga
   ProxyRequests off
   <Location />
      ProxyPass http://localhost:8081/
      ProxyPassReverse http://localhost:8081/
   </Location>
   ErrorLog /home/logs/error.log
   LogLevel warn
   CustomLog /home/logs/access.log combined
</VirtualHost>